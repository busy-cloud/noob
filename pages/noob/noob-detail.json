{
  "title": "物联小白详情",
  "template": "info",
  "toolbar": [
    {
      "icon": "edit",
      "type": "button",
      "label": "编辑",
      "action": {
        "type": "page",
        "app": "noob",
        "page": "noob-edit",
        "params_func": "return {id: data.id}"
      }
    },
    {
      "icon": "delete",
      "type": "button",
      "label": "删除",
      "confirm": "确认删除？",
      "action": {
        "type": "script",
        "script": "this.request.get('noob/noob/'+data.id+'/delete').subscribe(res=>{this.navigate('/page/noob/noob')})"
      }
    },
    {
      "icon": "sync",
      "type": "button",
      "label": "同步",
      "action": {
        "type": "script",
        "script": "this.request.get('noob/noob/'+data.id+'/sync').subscribe(res=>{})"
      }
    }
  ],
  "items": [
    {
      "key": "id",
      "label": "ID"
    },
    {
      "key": "name",
      "label": "名称"
    },
    {
      "key": "bsp",
      "label": "BSP"
    },
    {
      "key": "firmware",
      "label": "固件"
    },
    {
      "key": "imei",
      "label": "IMEI"
    },
    {
      "key": "imsi",
      "label": "IMSI"
    },
    {
      "key": "iccid",
      "label": "ICCID"
    },
    {
      "key": "created",
      "label": "创建日期",
      "type": "date"
    }
  ],
  "load_api": "noob/noob/:id",
  "children": [
    {
      "app": "noob",
      "page": "link",
      "params_func": "return {noob_id: params.id}"
    },
    {
      "app": "noob",
      "page": "device",
      "params_func": "return {noob_id: params.id}"
    }
  ]
}