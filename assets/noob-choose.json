{
  "title": "小白",
  "template": "table",
  "toolbar": [
    {
      "key": "keyword",
      "type": "text",
      "placeholder": "请输入关键字"
    },
    {
      "type": "button",
      "icon": "search",
      "label": "搜索",
      "action": {
        "type": "script",
        "script": "this.keyword=this.toolbar.value.keyword; this.search()"
      }
    }
  ],
  "keywords": [
    "id",
    "name",
    "imei",
    "iccid"
  ],
  "operators": [
    {
      "icon": "check",
      "label": "选择",
      "action": {
        "type": "script",
        "script": "this.modelRef.close(data)"
      }
    }
  ],
  "columns": [
    {
      "key": "id",
      "label": "ID"
    },
    {
      "key": "name",
      "label": "名称"
    },
    {
      "key": "imei",
      "label": "IMEI"
    },
    {
      "key": "iccid",
      "label": "ICCID"
    },
    {
      "key": "disabled",
      "label": "禁用"
    },
    {
      "key": "created",
      "label": "日期",
      "type": "date"
    }
  ],
  "search_api": "noob/noob/search"
}